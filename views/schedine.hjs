    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
</head>
<body>
    <div class="container">
        {{!-- navbar --}}
        <nav>
            <div class="nav-wrapper light-blue darken-4">
                <a href="#" data-target="slide-out" class="sidenav-trigger"><i class="material-icons">menu</i></a>
                <a href="/" class="brand-logo">LostBet</a>
                <ul id="nav-mobile" class="right hide-on-med-and-down">
                    <li><a href="/">Partite</a></li>
                    <li><a href="/classifica">Classifica</a></li>
                    <li><a href="/risultati">Risultati</a></li>
                    <li class="active"><a href="/schedine">Schedine</a></li>
                    <li><a href="/logout">LogOut</a></li>
                    <li><a href="#">C: {{crediti}}</a></li>
                </ul>
            </div>
        </nav>
        <ul id="slide-out" class="sidenav">
            <li><a href="/">Partite</a></li>
            <li><a href="/classifica">Classifica</a></li>
            <li><a href="/risultati">Risultati</a></li>
            <li><a href="/schedine">Schedine</a></li>
            <li><a href="/logout">LogOut</a></li>
            <li><a href="#">C: {{crediti}}</a></li>
        </ul>
    {{!-- navbar --}}
    {{#bets}}
    <div class="col s12 m7">
        <div class="card horizontal">
            <div class="card-stacked">
                <div class="card-content" style="padding: 10px">
                    <h6>{{mese}}</h6>
                    {{#partite}}
                    <p>{{s1}} - {{s2}} <b>@{{quota}}</b> <span class="secondary-content">{{p}}</span></p>
                    {{/partite}}
                    <b>Puntata: {{puntata}} - Bonus: {{bonus}} - Vincita Potenziale: {{vincitaPotenziale}}</b>
                </div>
                <div class="card-action" style="padding: 10px">
                    <a class="light-blue-text darken-4-text" href="/controllaScheda/{{id}}">CONTROLLA</a>
                    <a class="red-text darken-3" href="/rimuoviScheda/{{id}}">RIMUOVI</a>
                </div>
            </div>
        </div>
    </div>
    {{/bets}}

    </div>
    <script src="./sidenav.js"></script>
